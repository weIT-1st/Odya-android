<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="36dp">

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/tv_item_hot_place_rank"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/place_main_hot_place_rank"
        style="@style/detail1"
        android:paddingStart="8dp"
        android:paddingEnd="8dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/tv_item_hot_place_rank_title"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        style="@style/detail1"
        android:textColor="@color/label_normal"
        android:maxLines="1"
        android:ellipsize="end"
        android:layout_marginStart="8dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_item_hot_place_rank"
        app:layout_constraintEnd_toEndOf="parent"
        tools:text="해운대 해수욕장 해운대 해수욕장 해운대 해수욕장 해운대 해수욕장 해운대 해수욕장 "/>


</androidx.constraintlayout.widget.ConstraintLayout>
